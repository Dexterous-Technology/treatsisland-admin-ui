{
    "hosting": [{
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "target": "dev",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }, {
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "target": "prod",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }]
  }
  